{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "strict": true,
      "target": "esNext",
      "resolveJsonModule": true,
      "esModuleInterop": true,
    },
    "compileOnSave": true,
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  